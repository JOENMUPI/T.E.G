package LB;

import java.util.ArrayList;

public class TailTime {
	private ArrayList<TicketTime> tt = new ArrayList<TicketTime>();
	private int elements = 5;
	public void setTime(TicketTime t) { 
		if(this.checklong()) { this.tt.remove(0); }
		this.tt.add(t);
	}
	public void setElements(int elements) { this.elements = elements; }
	private Boolean checklong() {
		if(this.tt.size() == this.elements) { return true; }
		else { return false; }
	}
}